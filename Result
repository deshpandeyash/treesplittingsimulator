	10 -> 101 [label=101]
	10 -> 100 [label=100]
	101 -> 1011 [label=1011]
	101 -> 1010 [label=1010]
	0 -> 01 [label=01]
	0 -> 00 [label=00]
	01 -> 011 [label=011]
	01 -> 010 [label=010]
	011 -> 0111 [label=0111]
	011 -> 0110 [label=0110]
	0110 -> 01101 [label=01101]
	0110 -> 01100 [label=01100]
	00 -> 001 [label=001]
	00 -> 000 [label=000]
	001 -> 0011 [label=0011]
	001 -> 0010 [label=0010]
	0010 -> 00101 [label=00101]
	0010 -> 00100 [label=00100]
	00101 -> 001011 [label=001011]
	00101 -> 001010 [label=001010]
}
