	1010 [label=1 xlabel=1]
	100 [label=0 xlabel=0]
	0 [label=6 xlabel=2]
	Start -> 0 [label=0]
	01 [label=3 xlabel=2]
	011 [label=2 xlabel=2]
	0111 [label=0 xlabel=0]
	0110 [label=2 xlabel=2]
	01101 [label=1 xlabel=1]
	01100 [label=1 xlabel=1]
	010 [label=1 xlabel=1]
	00 [label=3 xlabel=2]
	001 [label=3 xlabel=2]
	0011 [label=1 xlabel=1]
	0010 [label=2 xlabel=2]
	00101 [label=2 xlabel=2]
	001011 [label=1 xlabel=1]
	001010 [label=1 xlabel=1]
	00100 [label=0 xlabel=0]
	000 [label=0 xlabel=0]
	1 -> 11 [label=11]
	1 -> 10 [label=10]
	11 -> 111 [label=111]
